array[10] integer array_t;

procedure main ()
    array_t arr;
    procedure (integer val x) consumer;
{
    call generate(arr);

    call for_each((integer val x) -> {
        write x;
        write " ";
    }, arr);
}

procedure generate (array_t arr)
    integer i;
{
    while i < 10 do
        arr[i] <- i;
        i <- i + 1;
    od
}

procedure for_each (procedure (integer val x) consumer, array_t arr)
    integer i;
{
    while i < 10 do
        call consumer(arr[i]);
        i <- i + 1;
    od
}
